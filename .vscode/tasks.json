{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"label": "Run",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"args": [
				"--bin",
				"vs-rs",
				"--features",
				"bevy/dynamic_linking"
			],
			"options": {
				"cwd": "./vs-rs/"
			}
		}
	]
}